        },
        "joins": [
          {
            "table": "m_api",
            "column": "*",
            "alias": "m_api",
            "type": "LEFT",
            "clauses": {
              "condition": "AND",
              "rules": [
                {
                  "table": "m_api",
                  "column": "id",
                  "operator": "equal",
                  "value": {
                    "table": "m_api_forward",
                    "column": "api_id"
                  },
                  "operation": "="
                }
              ]
            },
            "primary": "id"
          }
        ],
        "query": "SELECT m_api_forward.api_id, m_api.api_code, m_api.username, m_api.api_password, m_api.api_url\nFROM m_api_forward\nLEFT JOIN m_api AS m_api ON (m_api.id = m_api_forward.api_id)\nWHERE m_api.api_type = :P1 /* {{'AEPS'}} */",
        "params": [
          {
            "operator": "equal",
            "type": "expression",
            "name": ":P1",
            "value": "{{'AEPS'}}"
          }
        ],
        "wheres": {
          "condition": "AND",
          "rules": [
            {
              "id": "m_api.api_type",
              "field": "m_api.api_type",
              "type": "string",
              "operator": "equal",
              "value": "{{'AEPS'}}",
              "data": {
                "table": "m_api",
                "column": "api_type",
                "type": "text",
                "columnObj": {
                  "type": "string",
                  "maxLength": -5,
                  "primary": false,
                  "unique": false,
                  "nullable": true,
                  "name": "api_type"
                }
              },
              "operation": "="
            }
          ],
          "conditional": null,
          "valid": true
        }
      }
    },
