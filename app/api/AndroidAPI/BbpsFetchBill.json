    "$_SERVER": [
      {
        "type": "text",
        "name": "REMOTE_ADDR"
      }
    ]
  },
  "exec": {
    "steps": {
      "name": "",
      "module": "core",
      "action": "trycatch",
      "options": {
        "try": {
          "steps": [
            {
              "name": "getSalt",
              "module": "dbconnector",
              "action": "single",
              "options": {
                "connection": "postgres_gcpServer",
                "sql": {
                  "type": "SELECT",
                  "columns": [
                    {
                      "table": "m_users",
                      "column": "salt",
                      "alias": "saltvalue"
                    },
                    {
                      "table": "m_users",
                      "column": "user_tpin"
                    },
                    {
                      "table": "m_users",
                      "column": "id"
                    },
                    {
                      "table": "m_users",
                      "column": "md_id"
                    },
                    {
                      "table": "m_users",
                      "column": "ds_id"
                    }
                  ],
                  "table": {
                    "name": "m_users"
                  },
                  "joins": [],
                  "query": "SELECT salt AS saltvalue, user_tpin, id, md_id, ds_id\nFROM m_users\nWHERE user_type BETWEEN 1 AND 3 AND u_status = '1' AND blank2 = :P1 /* {{$_GET.username}} */",
                  "params": [
                    {
                      "operator": "equal",
                      "type": "expression",
                      "name": ":P1",
                      "value": "{{$_GET.username}}"
                    }
                  ],
                  "wheres": {
                    "condition": "AND",
                    "rules": [
                      {
                        "id": "m_users.user_type",
                        "field": "m_users.user_type",
                        "type": "double",
                        "operator": "between",
                        "value": [
                          1,
                          3
                        ],
                        "data": {
                          "table": "m_users",
                          "column": "user_type",
                          "type": "number"
                        },
                        "operation": "BETWEEN"
                      },
                      {
                        "id": "m_users.u_status",
                        "field": "m_users.u_status",
                        "type": "string",
                        "operator": "equal",
                        "value": "1",
                        "data": {
                          "table": "m_users",
                          "column": "u_status",
                          "type": "text"
                        },
                        "operation": "="
                      },
                      {
                        "id": "m_users.blank2",
                        "field": "m_users.blank2",
                        "type": "string",
                        "operator": "equal",
                        "value": "{{$_GET.username}}",
                        "data": {
                          "table": "m_users",
                          "column": "blank2",
                          "type": "text"
                        },
                        "operation": "="
                      }
                    ],
                    "conditional": null,
                    "valid": true
                  }
                }
              },
              "output": false,
              "meta": [
                {
                  "type": "text",
                  "name": "saltvalue"
                },
                {
                  "type": "text",
                  "name": "user_tpin"
                },
                {
                  "type": "number",
                  "name": "id"
                },
                {
                  "type": "text",
                  "name": "md_id"
                },
                {
                  "type": "text",
                  "name": "ds_id"
                }
              ],
              "outputType": "object"
            },
            {
              "name": "",
              "module": "core",
              "action": "condition",
              "options": {
                "if": "{{getSalt.saltvalue}}",
                "then": {
                  "steps": [
                    {
                      "name": "GetMdt",
                      "module": "dbconnector",
                      "action": "single",
                      "options": {
                        "connection": "postgres_gcpServer",
                        "sql": {
                          "type": "SELECT",
                          "columns": [
                            {
                              "table": "m_users",
                              "column": "id",
                              "alias": "countmdt",
                              "aggregate": "COUNT"
                            }
                          ],
                          "table": {
                            "name": "m_users"
                          },
                          "joins": [],
                          "groupBy": [],
                          "wheres": {
                            "condition": "AND",
                            "rules": [
                              {
                                "id": "m_users.id",
                                "field": "m_users.id",
                                "type": "double",
                                "operator": "equal",
                                "value": "{{getSalt.md_id.toNumber()}}",
                                "data": {
                                  "table": "m_users",
                                  "column": "id",
                                  "type": "number"
                                },
                                "operation": "="
                              },
                              {
                                "id": "m_users.u_status",
                                "field": "m_users.u_status",
                                "type": "string",
                                "operator": "equal",
                                "value": "1",
                                "data": {
                                  "table": "m_users",
                                  "column": "u_status",
                                  "type": "text"
                                },
                                "operation": "="
                              }
                            ],
                            "conditional": null,
                            "valid": true
                          },
                          "query": "SELECT COUNT(id) AS countmdt\nFROM m_users\nWHERE id = :P1 /* {{getSalt.md_id.toNumber()}} */ AND u_status = '1'",
                          "params": [
                            {
                              "operator": "equal",
                              "type": "expression",
                              "name": ":P1",
                              "value": "{{getSalt.md_id.toNumber()}}"
                            }
                          ]
                        }
                      },
                      "output": false,
                      "meta": [
                        {
                          "type": "number",
                          "name": "countmdt"
                        }
                      ],
                      "outputType": "object"
                    },
                    {
                      "name": "",
                      "module": "core",
                      "action": "condition",
                      "options": {
                        "if": "{{GetMdt.countmdt > 0 || getSalt.md_id.toNumber() == 0}}",
                        "then": {
                          "steps": [
                            {
                              "name": "GetDt",
                              "module": "dbconnector",
                              "action": "single",
                              "options": {
                                "connection": "postgres_gcpServer",
                                "sql": {
                                  "type": "SELECT",
                                  "columns": [
                                    {
                                      "table": "m_users",
                                      "column": "id",
                                      "alias": "countdt",
                                      "aggregate": "COUNT"
                                    }
                                  ],
                                  "table": {
                                    "name": "m_users"
                                  },
                                  "joins": [],
                                  "groupBy": [],
                                  "wheres": {
                                    "condition": "AND",
                                    "rules": [
                                      {
                                        "id": "m_users.id",
                                        "field": "m_users.id",
                                        "type": "double",
                                        "operator": "equal",
                                        "value": "{{getSalt.ds_id.toNumber()}}",
                                        "data": {
                                          "table": "m_users",
                                          "column": "id",
                                          "type": "number"
                                        },
                                        "operation": "="
                                      },
                                      {
                                        "id": "m_users.u_status",
                                        "field": "m_users.u_status",
                                        "type": "string",
                                        "operator": "equal",
                                        "value": "1",
                                        "data": {
                                          "table": "m_users",
                                          "column": "u_status",
                                          "type": "text"
                                        },
                                        "operation": "="
                                      }
                                    ],
                                    "conditional": null,
                                    "valid": true
                                  },
                                  "query": "SELECT COUNT(id) AS countdt\nFROM m_users\nWHERE id = :P1 /* {{getSalt.ds_id.toNumber()}} */ AND u_status = '1'",
                                  "params": [
                                    {
                                      "operator": "equal",
                                      "type": "expression",
                                      "name": ":P1",
                                      "value": "{{getSalt.ds_id.toNumber()}}"
                                    }
                                  ]
                                }
                              },
                              "output": false,
                              "meta": [
                                {
                                  "type": "number",
                                  "name": "countdt"
                                }
                              ],
                              "outputType": "object"
                            },
                            {
                              "name": "",
                              "module": "core",
                              "action": "condition",
                              "options": {
                                "if": "{{GetDt.countdt > 0 || getSalt.ds_id.toNumber() == 0}}",
                                "then": {
                                  "steps": [
                                    {
                                      "name": "identity",
                                      "module": "auth",
                                      "action": "login",
                                      "options": {
                                        "provider": "securityAndroid",
                                        "password": "{{$_GET.password.sha256(getSalt.saltvalue)}}",
                                        "username": "{{$_GET.username}}",
                                        "remember": ""
                                      },
                                      "output": false,
                                      "meta": []
                                    },
                                    {
                                      "name": "",
                                      "module": "core",
                                      "action": "condition",
                                      "options": {
                                        "if": "{{identity}}",
                                        "then": {
                                          "steps": [
                                            {
                                              "name": "ReqId",
                                              "module": "core",
                                              "action": "setvalue",
                                              "options": {
                                                "value": "{{NOW.md5(123456).substr(1, 9)}}"
                                              },
                                              "meta": [],
                                              "outputType": "text"
                                            },
                                            {
                                              "name": "GetOpCode",
                                              "module": "dbconnector",
                                              "action": "single",
                                              "options": {
                                                "connection": "postgres_gcpServer",
                                                "sql": {
                                                  "type": "SELECT",
                                                  "columns": [
                                                    {
                                                      "table": "t_op_api_rel",
                                                      "column": "api_code1",
                                                      "alias": "ouropcode"
                                                    },
                                                    {
                                                      "table": "t_op_api_rel",
                                                      "column": "api_code7",
                                                      "alias": "utilityopcode"
                                                    }
                                                  ],
                                                  "table": {
                                                    "name": "t_op_api_rel"
                                                  },
                                                  "joins": [],
                                                  "wheres": {
                                                    "condition": "AND",
                                                    "rules": [
                                                      {
                                                        "id": "t_op_api_rel.op_id",
                                                        "field": "t_op_api_rel.op_id",
                                                        "type": "double",
                                                        "operator": "equal",
                                                        "value": "{{$_GET.op_id}}",
                                                        "data": {
                                                          "table": "t_op_api_rel",
                                                          "column": "op_id",
                                                          "type": "number"
                                                        },
                                                        "operation": "="
                                                      }
                                                    ],
                                                    "conditional": null,
                                                    "valid": true
                                                  },
                                                  "query": "SELECT api_code1 AS ouropcode, api_code7 AS utilityopcode\nFROM t_op_api_rel\nWHERE op_id = :P1 /* {{$_GET.op_id}} */",
                                                  "params": [
                                                    {
                                                      "operator": "equal",
                                                      "type": "expression",
                                                      "name": ":P1",
                                                      "value": "{{$_GET.op_id}}"
                                                    }
                                                  ]
                                                }
                                              },
                                              "output": true,
                                              "meta": [
                                                {
                                                  "type": "text",
                                                  "name": "ouropcode"
                                                },
                                                {
                                                  "type": "text",
                                                  "name": "utilityopcode"
                                                }
                                              ],
                                              "outputType": "object"
                                            },
                                            {
                                              "name": "api",
                                              "module": "api",
                                              "action": "send",
                                              "options": {
                                                "url": "https://utilityhub.app/API/FetchBill",
                                                "params": {
                                                  "UserID": "{{214}}",
                                                  "Token": "{{'879cfb7ae02677efe36abde00dc77fac'}}",
                                                  "Account": "{{$_GET.account}}",
                                                  "Amount": "0",
                                                  "SPKey": "{{GetOpCode.utilityopcode}}",
                                                  "APIRequestID": "{{ReqId}}",
                                                  "Optional1": "",
                                                  "Optional2": "",
                                                  "Optional3": "",
                                                  "Optional4": "",
                                                  "GEOCode": "{{'23.8530,87.9727'}}",
                                                  "CustomerNumber": "",
                                                  "Pincode": "{{243601}}",
                                                  "Format": "1"
                                                }
                                              },
                                              "output": true
                                            }
                                          ]
                                        },
                                        "else": {
                                          "steps": {
                                            "name": "Resp",
                                            "module": "core",
                                            "action": "setvalue",
                                            "options": {
                                              "value": "Password Mismatched"
                                            },
                                            "meta": [],
                                            "output": true,
                                            "outputType": "text"
                                          }
                                        }
                                      },
                                      "outputType": "boolean"
                                    }
                                  ]
                                },
                                "else": {
                                  "steps": {
                                    "name": "Resp",
                                    "module": "core",
                                    "action": "setvalue",
                                    "options": {
                                      "value": "Your Distributor(DT) inactive, Please contact technical support team"
                                    },
                                    "meta": [],
                                    "outputType": "text",
                                    "output": true
                                  }
                                }
                              },
                              "outputType": "boolean"
                            }
                          ]
                        },
                        "else": {
                          "steps": {
                            "name": "Resp",
                            "module": "core",
                            "action": "setvalue",
                            "options": {
                              "value": "Your Master Distributor(MDT) inactive, Please contact technical support team"
                            },
                            "meta": [],
                            "output": true,
                            "outputType": "text"
                          }
                        }
                      },
                      "outputType": "boolean"
                    }
                  ]
                },
                "else": {
                  "steps": {
                    "name": "Resp",
                    "module": "core",
                    "action": "setvalue",
                    "options": {
                      "value": "User does not exists or user inactive, Please contact technical support team"
                    },
                    "meta": [],
                    "outputType": "text",
                    "output": true
                  }
                }
              },
              "outputType": "boolean",
              "output": false
            }
          ]
        },
        "catch": {
          "steps": {
            "name": "Resp",
            "module": "core",
            "action": "setvalue",
            "options": {
              "value": "Do not even try to do this, we are watching & recording your wrong activity!"
            },
            "meta": [],
            "output": true,
            "outputType": "text"
          }
        }
      }
    }
  }
}