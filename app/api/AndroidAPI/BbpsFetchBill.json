{
  "meta": {
    "options": {
      "linkedFile": "/views/login.ejs",
      "linkedForm": "formLogin"
    },
    "$_GET": [
      {
        "type": "text",
        "options": {
          "rules": {
            "core:required": {}
          }
        },
        "name": "username"
      },
      {
        "type": "text",
        "options": {
          "rules": {
            "core:required": {}
          }
        },
        "name": "password"
      },
      {
        "type": "text",
        "options": {
          "rules": {
            "core:required": {}
          }
        },
        "name": "op_id"
      },
      {
        "type": "text",
        "options": {
          "rules": {
            "core:required": {}
          }
        },
