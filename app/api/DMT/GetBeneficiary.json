{
  "meta": {
    "options": {
      "linkedFile": "/views/dmt-service.ejs",
      "linkedForm": "formGetBeneficiary"
    },
    "$_POST": [
      {
        "type": "number",
        "fieldName": "CustMobile",
        "options": {
          "rules": {
            "core:required": {
              "param": ""
            },
            "core:number": {}
          }
        },
        "name": "CustMobile"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "",
        "module": "auth",
        "action": "restrict",
        "options": {
          "provider": "security",
          "permissions": [
            "write"
          ],
          "loginUrl": "/login",
          "forbiddenUrl": "/403-forbidden"
        }
      },
      {
        "name": "apiGetBeneficiary",
        "module": "api",
        "action": "send",
        "options": {
          "url": "https://apihub.moneyart.in/ValidateRemitter",
          "params": {
            "token": "{{'a20818a9-a062-40ec-ac57-29ee18b6e124'}}",
            "mobileNo": "{{$_POST.CustMobile}}"
          },
          "method": "POST"
        },
        "output": true
      },
      {
        "name": "",
        "module": "core",
        "action": "condition",
        "options": {
          "if": "{{apiGetBeneficiary.data.StatusCode==2}}",
          "then": {
            "steps": [
              {
                "name": "api",
                "module": "api",
                "action": "send",
                "options": {
                  "url": "https://apihub.moneyart.in/RemitterSendOtp",
                  "params": {
                    "token": "{{'a20818a9-a062-40ec-ac57-29ee18b6e124'}}",
                    "mobileNo": "{{$_POST.CustMobile}}"
                  },
                  "method": "POST"
                },
                "output": false
              },
              {
                "name": "apiRemOTP",
                "module": "core",
                "action": "setvalue",
                "options": {
                  "value": "{{api.data}}"
                },
                "meta": [],
                "outputType": "text",
                "output": true
              }
            ]
          }
        },
        "outputType": "boolean"
      }
    ]
  }
}