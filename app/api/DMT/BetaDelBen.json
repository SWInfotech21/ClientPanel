{
  "meta": {
    "options": {
      "linkedFile": "/views/dmt-service-beta.ejs",
      "linkedForm": "formDelBen"
    },
    "$_POST": [
      {
        "type": "text",
        "fieldName": "RemMobileDel",
        "options": {
          "rules": {
            "core:required": {
              "param": ""
            }
          }
        },
        "name": "RemMobileDel"
      },
      {
        "type": "text",
        "fieldName": "BenIdDel",
        "options": {
          "rules": {
            "core:required": {
              "param": ""
            }
          }
        },
        "name": "BenIdDel"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "",
        "module": "auth",
        "action": "restrict",
        "options": {
          "provider": "security",
          "permissions": [
            "write"
          ],
          "loginUrl": "/login",
          "forbiddenUrl": "/403-forbidden"
        }
      },
      {
        "name": "AuthCode",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "65c43ccd6a7b41c884"
        },
        "meta": [],
        "outputType": "text"
      },
      {
        "name": "MessageCode",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "E06013"
        },
        "meta": [],
        "outputType": "text"
      },
      {
        "name": "ReqId",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{NOW.md5(123456).substr(1, 9)}}"
        },
        "meta": [],
        "outputType": "text"
      },
      {
        "name": "apiDelBen",
        "module": "api",
        "action": "send",
        "options": {
          "url": "https://ezymoney.myezypay.in/RemitMoney/mtransfer",
          "method": "POST",
          "data": {
            "msg": "{{MessageCode+'~'+AuthCode+'~'+ReqId+'~'+$_POST.RemMobileDel+'~'+$_POST.BenIdDel+'~0~0'}}"
          },
          "dataType": "x-www-form-urlencoded"
        },
        "output": false
      },
      {
        "name": "DelBenMobile",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{apiDelBen.data.split('~')[2]}}"
        },
        "meta": [],
        "outputType": "text",
        "output": true
      },
      {
        "name": "DelBenName",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{apiDelBen.data.split('~')[3]}}"
        },
        "meta": [],
        "outputType": "text",
        "output": true
      },
      {
        "name": "VerifyRefNo",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{apiDelBen.data.split('~')[4]}}"
        },
        "meta": [],
        "outputType": "text",
        "output": true
      },
      {
        "name": "DelStatusCode",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{apiDelBen.data.split('~')[5]}}"
        },
        "meta": [],
        "outputType": "text",
        "output": true
      },
      {
        "name": "DelMessage",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{apiDelBen.data.split('~')[6]}}"
        },
        "meta": [],
        "outputType": "text",
        "output": true
      }
    ]
  }
}