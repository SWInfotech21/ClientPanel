{
  "meta": {
    "options": {
      "linkedFile": "/views/dmt-service.ejs",
      "linkedForm": "formAddBen"
    },
    "$_POST": [
      {
        "type": "text",
        "fieldName": "BenSenderId",
        "options": {
          "rules": {
            "core:required": {
              "param": ""
            }
          }
        },
        "name": "BenSenderId"
      },
      {
        "type": "number",
        "fieldName": "BenMobileAdd",
        "options": {
          "rules": {
            "core:required": {
              "param": ""
            },
            "core:number": {
              "param": ""
            }
          }
        },
        "name": "BenMobileAdd"
      },
      {
        "type": "text",
        "fieldName": "BenACAdd",
        "options": {
          "rules": {
            "core:required": {
              "param": ""
            }
          }
        },
        "name": "BenACAdd"
      },
      {
        "type": "text",
        "fieldName": "BenNameAdd",
        "options": {
          "rules": {
            "core:required": {
              "param": ""
            }
          }
        },
        "name": "BenNameAdd"
      },
      {
        "type": "text",
        "fieldName": "BenIFSCAdd",
        "options": {
          "rules": {
            "core:required": {
              "param": ""
            }
          }
        },
        "name": "BenIFSCAdd"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "",
        "module": "auth",
        "action": "restrict",
        "options": {
          "provider": "security",
          "permissions": [
            "write"
          ],
          "loginUrl": "/login",
          "forbiddenUrl": "/403-forbidden"
        },
        "output": false
      },
      {
        "name": "identity",
        "module": "auth",
        "action": "identify",
        "options": {
          "provider": "security"
        },
        "output": false,
        "meta": []
      },
      {
        "name": "apiAddBen",
        "module": "api",
        "action": "send",
        "options": {
          "url": "https://apihub.moneyart.in/AddRecipients",
          "params": {
            "token": "{{'a20818a9-a062-40ec-ac57-29ee18b6e124'}}",
            "senderId": "{{$_POST.BenSenderId}}",
            "name": "{{$_POST.BenNameAdd}}",
            "mobileno": "{{$_POST.BenMobileAdd}}",
            "accountno": "{{$_POST.BenACAdd}}",
            "ifsc": "{{$_POST.BenIFSCAdd}}"
          },
          "method": "POST"
        },
        "output": true
      },
      {
        "name": "",
        "module": "core",
        "action": "condition",
        "options": {
          "if": "{{apiAddBen.data.StatusCode==1}}",
          "then": {
            "steps": {
              "name": "AddBen",
              "module": "dbupdater",
              "action": "insert",
              "options": {
                "connection": "postgres_gcpServer",
                "sql": {
                  "type": "insert",
                  "values": [
                    {
                      "table": "dmt_beneficiary",
                      "column": "ben_name",
                      "type": "text",
                      "value": "{{$_POST.BenNameAdd}}"
                    },
                    {
                      "table": "dmt_beneficiary",
                      "column": "ben_mobile",
                      "type": "text",
                      "value": "{{$_POST.BenMobileAdd}}"
                    },
                    {
                      "table": "dmt_beneficiary",
                      "column": "ben_ac_no",
                      "type": "text",
                      "value": "{{$_POST.BenACAdd}}"
                    },
                    {
                      "table": "dmt_beneficiary",
                      "column": "ben_ifsc",
                      "type": "text",
                      "value": "{{$_POST.BenIFSCAdd}}"
                    },
                    {
                      "table": "dmt_beneficiary",
                      "column": "remitter_id",
                      "type": "text",
                      "value": "{{$_POST.BenSenderId}}"
                    },
                    {
                      "table": "dmt_beneficiary",
                      "column": "ben_verify",
                      "type": "boolean",
                      "value": "{{false}}"
                    },
                    {
                      "table": "dmt_beneficiary",
                      "column": "status",
                      "type": "text",
                      "value": "{{'Active'}}"
                    },
                    {
                      "table": "dmt_beneficiary",
                      "column": "created_by",
                      "type": "text",
                      "value": "{{identity}}"
                    },
                    {
                      "table": "dmt_beneficiary",
                      "column": "created_on",
                      "type": "datetime",
                      "value": "{{NOW}}"
                    },
                    {
                      "table": "dmt_beneficiary",
                      "column": "ben_id",
                      "type": "text",
                      "value": "{{apiAddBen.data.Data.RecipientCode}}"
                    }
                  ],
                  "table": "dmt_beneficiary",
                  "returning": "id",
                  "query": "INSERT INTO dmt_beneficiary\n(ben_name, ben_mobile, ben_ac_no, ben_ifsc, remitter_id, ben_verify, status, created_by, created_on, ben_id) VALUES (:P1 /* {{$_POST.BenNameAdd}} */, :P2 /* {{$_POST.BenMobileAdd}} */, :P3 /* {{$_POST.BenACAdd}} */, :P4 /* {{$_POST.BenIFSCAdd}} */, :P5 /* {{$_POST.BenSenderId}} */, :P6 /* {{false}} */, :P7 /* {{'Active'}} */, :P8 /* {{identity}} */, :P9 /* {{NOW}} */, :P10 /* {{apiAddBen.data.Data.RecipientCode}} */)",
                  "params": [
                    {
                      "name": ":P1",
                      "type": "expression",
                      "value": "{{$_POST.BenNameAdd}}"
                    },
                    {
                      "name": ":P2",
                      "type": "expression",
                      "value": "{{$_POST.BenMobileAdd}}"
                    },
                    {
                      "name": ":P3",
                      "type": "expression",
                      "value": "{{$_POST.BenACAdd}}"
                    },
                    {
                      "name": ":P4",
                      "type": "expression",
                      "value": "{{$_POST.BenIFSCAdd}}"
                    },
                    {
                      "name": ":P5",
                      "type": "expression",
                      "value": "{{$_POST.BenSenderId}}"
                    },
                    {
                      "name": ":P6",
                      "type": "expression",
                      "value": "{{false}}"
                    },
                    {
                      "name": ":P7",
                      "type": "expression",
                      "value": "{{'Active'}}"
                    },
                    {
                      "name": ":P8",
                      "type": "expression",
                      "value": "{{identity}}"
                    },
                    {
                      "name": ":P9",
                      "type": "expression",
                      "value": "{{NOW}}"
                    },
                    {
                      "name": ":P10",
                      "type": "expression",
                      "value": "{{apiAddBen.data.Data.RecipientCode}}"
                    }
                  ]
                }
              },
              "meta": [
                {
                  "name": "identity",
                  "type": "text"
                },
                {
                  "name": "affected",
                  "type": "number"
                }
              ]
            }
          }
        },
        "outputType": "boolean"
      }
    ]
  }
}